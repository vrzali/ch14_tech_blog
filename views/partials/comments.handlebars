<div class="comment">
  {{#each this}}
    <div>
        <h3></h3>
        <section>
            <div class="text">
                {{comment_text}}
            </div>
            <div class="meta">
                -{{user.username}}, {{format_date created_at}}
            </div>
        </section>
    </div>
  {{/each}}
</div>


